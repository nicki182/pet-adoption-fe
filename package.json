    "@storybook/testing-library": "^0.0.9",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/react": "17.0.38",
    "babel-jest": "27.4.5",
    "babel-loader": "^8.2.4",
    "cypress": "^9.5.2",
    "eslint": "8.5.0",
